{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/enriched_metadata.schema.json",
  "title": "Enriched Metadata",
  "type": "object",
  "required": ["domain_definition"],
  "properties": {
    "domain_definition": {
      "type": "object",
      "required": [
        "min_x", "max_x",
        "min_y", "max_y",
        "min_z", "max_z",
        "nx", "ny", "nz"
      ],
      "properties": {
        "min_x": { "type": "number" },
        "max_x": { "type": "number" },
        "min_y": { "type": "number" },
        "max_y": { "type": "number" },
        "min_z": { "type": "number" },
        "max_z": { "type": "number" },
        "nx": { "type": "integer", "minimum": 1 },
        "ny": { "type": "integer", "minimum": 1 },
        "nz": { "type": "integer", "minimum": 1 }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}



