{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/mutation_pathways_log.schema.json",
  "title": "Mutation Pathways Log",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["step_index", "pressure_mutated", "triggered_by"],
    "properties": {
      "step_index": {
        "type": "integer",
        "minimum": 0
      },
      "pressure_mutated": {
        "type": "boolean"
      },
      "triggered_by": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "additionalProperties": false
  }
}



